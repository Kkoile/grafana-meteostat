{
  "type": "datasource",
  "name": "Meteostat",
  "id": "meteostat-meteostat-datasource",
  "metrics": true,
  "info": {
    "description": "Access historic weather data using the meteostat API",
    "author": {
      "name": "Meteostat",
      "url": "https://github.com/meteostat/meteostat-grafana"
    },
    "keywords": [
      "meteostat",
      "weather",
      "temperature"
    ],
    "logos": {
      "small": "img/logo.svg",
      "large": "img/logo.svg"
    },
    "links": [
      {
        "name": "Website",
        "url": "http://meteostat.net/"
      },
      {
        "name": "Github",
        "url": "https://github.com/meteostat/meteostat-grafana"
      }
    ],
    "screenshots": [
      {
        "name": "Meteostat API",
        "path": "img/screenshot.png"
      }
    ],
    "version": "%VERSION%",
    "updated": "%TODAY%"
  },
  "dependencies": {
    "grafanaDependency": "7.3.x",
    "plugins": []
  },
  "routes": [
    {
      "path": "meteostat",
      "url": "https://api.meteostat.net",
      "headers": [
        {
          "name": "x-api-key",
          "content": "{{ .SecureJsonData.apiKey }}"
        }
      ]
    }
  ]
}
